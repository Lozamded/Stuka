<p id="notice"><%= notice %></p>

<h1>Socios</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Rut</th>
      <th>Aporte</th>
      <th>Fecha nacimiento</th>
      <th>Email</th>
      <th>Direccion</th>
      <th>Comuna</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.nombre %></td>
        <td><%= socio.rut %></td>
        <td><%= socio.aporte %></td>
        <td><%= socio.fecha_nacimiento %></td>
        <td><%= socio.email %></td>
        <td><%= socio.direccion %></td>
        <td><%= socio.comuna %></td>
        <td><%= link_to 'Show', socio %></td>
        <td><%= link_to 'Edit', edit_socio_path(socio) %></td>
        <td><%= link_to 'Destroy', socio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Socio', new_socio_path %>
